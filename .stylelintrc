{
  'extends': 'stylelint-config-recommended',
  'ignoreFiles': ['**/node_modules'],
  'plugins': ['stylelint-no-unsupported-browser-features'],
    'rules': {
      'plugin/no-unsupported-browser-features': [
        true,
        {
          'browsers': ['last 2 versions'],
          'severity': 'warning'
        }
      ]
    }
}